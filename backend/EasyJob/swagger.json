  "components": {
    "schemas": {
      "Address": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "latitude": {
            "type": "number",
            "format": "float"
          },
          "longitude": {
            "type": "number",
            "format": "float"
          },
          "country": {
            "type": "string",
            "nullable": true
          },
          "city": {
            "type": "string",
            "nullable": true
          },
          "addressLine": {
            "type": "string",
            "nullable": true
          },
          "zip": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CategoryResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "categoryName": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "JobDuration": {
        "type": "object",
        "properties": {
          "amount": {
            "type": "integer",
            "format": "int32"
          },
          "durationType": {
            "$ref": "#/components/schemas/JobDurationType"
          }
        },
        "additionalProperties": false
      },
      "JobDurationType": {
        "enum": [
          "Hours",
          "Days",
          "Months"
        ],
        "type": "string"
      },
      "JobResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "numberOfEmployeesRequired": {
            "type": "integer",
            "format": "int32"
          },
          "location": {
            "$ref": "#/components/schemas/Address"
          },
          "duration": {
            "$ref": "#/components/schemas/JobDuration"
          },
          "price": {
            "$ref": "#/components/schemas/Price"
          },
          "urgency": {
            "$ref": "#/components/schemas/JobUrgency"
          }
        },
        "additionalProperties": false
      },
      "JobUrgency": {
        "enum": [
          "Normal",
          "Hight",
          "Urgent"
        ],
        "type": "string"
      },
      "Price": {
        "type": "object",
        "properties": {
          "currencyCode": {
            "type": "string",
            "nullable": true
          },
          "priceType": {
            "$ref": "#/components/schemas/PriceType"
          },
          "minPrice": {
            "type": "integer",
            "format": "int32"
          },
          "maxPrice": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "PriceType": {
        "enum": [
          "Hourly",
          "FixedPrice"
        ],
        "type": "string"
      }
    }
  }
}